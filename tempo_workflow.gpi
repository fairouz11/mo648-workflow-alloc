set autoscale 	# escala os eixos do gráfico automaticamente
unset log	# apaga qualquer log anterior
unset label	# apaga qualquer rótulo/label anterior

set style data lines
set xlabel "Workflow"
set ylabel "Tempo Médio (hora)"
set title "Tempo Médio x Workflow"

#seta a posição para a key
set key left top 

# gera o gráfico de saída com uma imagem png
set terminal png
set output "resultado_pso/tempo_workflow.png"

plot "resultado_pso/tempo_workflow.dat" title "Tempo médio" with errorlines
